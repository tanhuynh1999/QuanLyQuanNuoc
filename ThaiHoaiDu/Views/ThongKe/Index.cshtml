
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/QuanLy.cshtml";
    ThaiHoaiDu.Models.NguoiDung nd = (ThaiHoaiDu.Models.NguoiDung)Session["TaiKhoan"];
    if(nd != null && nd.VaiTro == false)
    {
        Response.Redirect("/QuanLy/Index");
    }
}


<div class="inner-block">
    <div class="product-block">
        <div class="pro-head" style="width: 100%; height: 1400px">
            <div class="col-lg-6">
                @Html.Action("tkNgay", "ThongKe")
            </div>
            <div class="col-lg-6">
                @Html.Action("tkTuan", "ThongKe")
            </div>

            <div class="col-lg-6">
                <br />
                @Html.Action("tkThang", "ThongKe")
            </div>

            <div class="col-lg-6">
                <br />
                @Html.Action("tkNam", "ThongKe")
            </div>
        </div>
    </div>
</div>
